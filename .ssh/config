	User root

# Let's setup our jump hosts
#
Host admin-prd.daas.corp.telstra.com
	Hostname admin-prd.daas.corp.telstra.com
	User d872288

Host admin-dv3.daas.corp.telstra.com
	Hostname admin-dv3.daas.corp.telstra.com
	User d872288

Host admin-dv2.daas.corp.telstra.com
	Hostname admin-dv2.daas.corp.telstra.com
	User d872288

Host admin-svcprd.daas.corp.telstra.com
	Hostname admin-svcprd.daas.corp.telstra.com
	User d872288

Host kafka-0*
	ProxyJump admin-prd.daas.corp.telstra.com

# OAN Model
Host 10.12.152.*
	User localadmin

Host 10.12.152.11
	DynamicForward 2004

# OAN Model CIMC
Host 10.12.155.*
	User admin
	ProxyJump 10.12.152.11

# OAN Production
Host 172.16.3.*
	User ubuntu
	ProxyJump admin.analytics.in.telstra.com.au
	IdentityFile ~/oan/maas-provisioning-rsa

Host 172.16.8.*
	User ubuntu
	ProxyJump admin.analytics.in.telstra.com.au
	IdentityFile ~/oan/maas-provisioning-rsa

Host 172.16.9.*
	User ubuntu
	ProxyJump admin.analytics.in.telstra.com.au
	IdentityFile ~/oan/maas-provisioning-rsa

Host 10.195.28.*
	User ubuntu
	IdentityFile ~/oan/maas-provisioning-rsa

Host *.oan
	User ubuntu
	ProxyJump admin.analytics.in.telstra.com.au
	IdentityFile ~/oan/maas-provisioning-rsa

# OAN Jump Box
Host admin.analytics.in.telstra.com.au
	User ubuntu
	DynamicForward 2003
	IdentityFile ~/oan/maas-provisioning-rsa

# OAN Production CIMC
Host 10.195.24.*
	User admin
	ProxyJump admin.analytics.in.telstra.com.au

# OAN Production CIMC via CMN
Host 10.195.31.*
	User ubuntu
	ProxyJump sharedboss.cmn.corp.telstra.com

# OAN Production when on VPN
Host oanvpn
	HostName admin.analytics.in.telstra.com.au
	User ubuntu
	ProxyJump d872288@10.60.105.125
	DynamicForward 2003
	IdentityFile ~/oan/maas-provisioning-rsa

# Fall through
Host *telstra*
	User d872288
